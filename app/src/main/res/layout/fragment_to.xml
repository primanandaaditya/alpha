<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    tools:context=".fragments.to.ToFragment">

   <LinearLayout

       android:layout_margin="20dp"
       android:orientation="vertical"
       android:layout_width="match_parent"
       android:layout_height="match_parent">

       <androidx.cardview.widget.CardView
           app:cardCornerRadius="10dp"
           android:layout_width="match_parent"
           android:layout_height="wrap_content">

           <LinearLayout
               android:padding="10dp"
               android:orientation="horizontal"
               android:layout_width="match_parent"
               android:layout_height="wrap_content">

               <LinearLayout

                   android:layout_weight="4"
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">

                   <ImageView
                       android:scaleType="fitCenter"
                       app:srcCompat="@drawable/ic_akun"
                       android:layout_width="match_parent"
                       android:layout_height="match_parent">

                   </ImageView>

               </LinearLayout>

               <LinearLayout
                   android:orientation="vertical"

                   android:layout_weight="1"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content">

                   <TextView
                       android:layout_marginBottom="5dp"
                       android:textStyle="bold"
                       android:id="@+id/tv_nama"
                       android:text="Elizabeth Tasya A."
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content">

                   </TextView>

                   <TextView

                       android:textSize="12dp"
                       android:text="Tryout UTBK Saintek 84"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content">

                   </TextView>

                   <TextView

                       android:textSize="12dp"
                       android:text="Score : 600"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content">

                   </TextView>

                   <TextView

                       android:textSize="12dp"
                       android:text="Ranking : 7"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content">

                   </TextView>

               </LinearLayout>

           </LinearLayout>

       </androidx.cardview.widget.CardView>

       <View
           android:layout_width="match_parent"
           android:layout_height="5dp">
       </View>

       <androidx.cardview.widget.CardView
           android:background="@color/putih"
           app:cardCornerRadius="10dp"
           android:layout_width="match_parent"
           android:layout_height="wrap_content">

           <androidx.appcompat.widget.SearchView

               android:id="@+id/sv"
               app:queryHint="Pencarian"
               app:defaultQueryHint="Pencarian"
               android:layout_width="match_parent"
               android:layout_height="wrap_content">

           </androidx.appcompat.widget.SearchView>

       </androidx.cardview.widget.CardView>

       <View
           android:layout_width="match_parent"
           android:layout_height="5dp">
       </View>

       <androidx.cardview.widget.CardView
           android:background="@color/putih"
           app:cardCornerRadius="10dp"
           android:layout_width="match_parent"
           android:layout_height="wrap_content">

           <LinearLayout
               android:orientation="vertical"
               android:layout_width="match_parent"
               android:layout_height="wrap_content">

               <LinearLayout
                   android:padding="10dp"
                   android:orientation="horizontal"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content">

                   <LinearLayout
                       android:layout_weight="1"
                       android:orientation="vertical"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content">

                       <TextView
                           android:text="@string/mata_pelajaran"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content">

                       </TextView>

                   </LinearLayout>

                   <LinearLayout
                       android:layout_weight="1"
                       android:orientation="vertical"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content">

                       <Spinner
                           android:id="@+id/sp_mata_pelajaran"
                           android:entries="@array/sp_kelas"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content">

                       </Spinner>
                   </LinearLayout>

               </LinearLayout>

               <GridView
                   android:id="@+id/gridview"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:columnWidth="100dp"
                   android:gravity="center"
                   android:layout_marginBottom="20dp"
                   android:numColumns="4" />

           </LinearLayout>



       </androidx.cardview.widget.CardView>


   </LinearLayout>

</FrameLayout>